#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/ext_power.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/pointing.h>

#include <behaviors/num_word.dtsi>

#define LILY58_STANDARD_LABELS 1
#include "zmk-helpers/helper.h"
#include "zmk-helpers/key-labels/lily58.h"

#define ___ &trans

#define KEYS_L LN0 LN1 LN2 LN3 LN4 LN5 \
              LT0 LT1 LT2 LT3 LT4 LT5 \
              LM0 LM1 LM2 LM3 LM4 LM5 \
              LB0 LB1 LB2 LB3 LB4 LB5

#define KEYS_R RN0 RN1 RN2 RN3 RN4 RN5 \
              RT0 RT1 RT2 RT3 RT4 RT5 \
              RM0 RM1 RM2 RM3 RM4 RM5 \
              RB0 RB1 RB2 RB3 RB4 RB5

#define THUMBS LH0 LH1 LH2 LH3 RH0 RH1 RH2 RH3

#define BASE 0
#define NUM 1
#define SYMBOL 2
#define NAV 3
#define UMLAUT 4
#define MOUSE 5
#define MEDIA 6
#define SYS 7

/ {
    behaviors {
        bspcdel: backspace_delete {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp BSPC>, <&kp DEL>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
        lt: layer_tap {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            quick-tap-ms = <125>;
            tapping-term-ms = <150>;
            bindings = <&mo>, <&kp>;
            display-name = "Layer-Tap";
        };
        ht: hold_tap {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping_term_ms = <200>;
            quick_tap_ms = <200>;
            flavor = "tap-preferred";
            bindings = <&kp>, <&kp>;
        };
    };
};

/ {
    combos {
        compatible = "zmk,combos";

        combo_capsword {
            timeout-ms = <50>;
            key-positions = <LH0 RH0>;
            bindings = <&caps_word>;
        };

        combo_numword {
            timeout-ms = <50>;
            key-positions = <LH1 RH0>;
            bindings = <&num_word NUM>;
        };

        combo_quote {
            timeout-ms = <20>;
            key-positions = <RM1 RB1>;
            bindings = <&kp SINGLE_QUOTE>;
        };

        combo_double_quote {
            timeout-ms = <20>;
            key-positions = <RM2 RB2>;
            bindings = <&kp DOUBLE_QUOTES>;
        };

        combo_semicolon {
            timeout-ms = <20>;
            key-positions = <RM3 RB3>;
            bindings = <&kp SEMICOLON>;
        };

        combo_colon {
            timeout-ms = <20>;
            key-positions = <RM4 RB4>;
            bindings = <&kp COLON>;
        };

        combo_minus {
            timeout-ms = <20>;
            key-positions = <LM3 LB3>;
            bindings = <&kp MINUS>;
        };

        combo_asterisk {
            timeout-ms = <20>;
            key-positions = <LM2 LB2>;
            bindings = <&kp ASTERISK>;
        };

        combo_equal {
            timeout-ms = <20>;
            key-positions = <LM1 LB1>;
            bindings = <&kp EQUAL>;
        };

        combo_underscore {
            timeout-ms = <20>;
            key-positions = <LM4 LB4>;
            bindings = <&kp UNDERSCORE>;
        };

        combo_tab {
            timeout-ms = <20>;
            key-positions = <LM4 LM3>;
            bindings = <&kp TAB>;
        };
    };
};

ZMK_HOLD_TAP(hml,
    flavor = "balanced";
    tapping-term-ms = <280>;
    quick-tap-ms = <175>;
    require-prior-idle-ms = <150>;
    hold-trigger-key-positions = <KEYS_R THUMBS>;
    hold-trigger-on-release;
    bindings = <&kp>, <&kp>;
)

ZMK_HOLD_TAP(hmr,
    flavor = "balanced";
    tapping-term-ms = <280>;
    quick-tap-ms = <175>;
    require-prior-idle-ms = <150>;
    hold-trigger-key-positions = <KEYS_L THUMBS>;
    hold-trigger-on-release;
    bindings = <&kp>, <&kp>;
)

ZMK_HOLD_TAP(hmd,
    flavor = "balanced";
    tapping-term-ms = <280>;
    quick-tap-ms = <175>;
    require-prior-idle-ms = <150>;
    hold-trigger-key-positions = <KEYS_L THUMBS>;
    hold-trigger-on-release;
    bindings = <&kp>, <&bspcdel>;
)

